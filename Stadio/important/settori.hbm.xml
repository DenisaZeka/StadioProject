<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.project.bean.Settore" table="settori">  
    <id name="id_settore">  
     	<generator class="increment"></generator>  
    </id>  
    <property name="nome_settore" column="nome_settore"></property>  
    <property name="moltiplicatore_tariffa_settore" column="moltiplicatore_tariffa_settore"></property>  
        
  </class>  
            
 </hibernate-mapping> 

